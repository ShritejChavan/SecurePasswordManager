<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd" id="WebApp_ID" version="6.0">
  <display-name>PasswordManager</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>Validator</display-name>
    <servlet-name>Validator</servlet-name>
    <servlet-class>Validate.Validator</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Validator</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>RegisterUser</display-name>
    <servlet-name>RegisterUser</servlet-name>
    <servlet-class>Validate.RegisterUser</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RegisterUser</servlet-name>
    <url-pattern>/registerUser</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>Logout</display-name>
    <servlet-name>Logout</servlet-name>
    <servlet-class>Operations.Logout</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Logout</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>Context</display-name>
    <servlet-name>Context</servlet-name>
    <servlet-class>Operators.Password_Manager_Context</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Context</servlet-name>
    <url-pattern>/passwordContext</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>AddPasswordToDb</display-name>
    <servlet-name>AddPasswordToDb</servlet-name>
    <servlet-class>Operations.AddPasswordToDb</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddPasswordToDb</servlet-name>
    <url-pattern>/addPassword</url-pattern>
  </servlet-mapping>
</web-app>